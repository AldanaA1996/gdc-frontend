        {id: "7" }, //"albaÃ±ileria"
        {id: "8" }, //"electricidad"
        {id: "9" }, //"herreria"


    ];
    return [ { params: { route: undefined } },
    { params: { route: "login" } },
    { params: { route: "home" }},
    { params: { route: "inventario" } },
    { params: { route: "addTool"} },
    { params: { route: "departments" } },
